	SetVehicleVirtualWorld(Car[series][number],0);

	VehicleStatus[Car[series][number]] = false;

	for(new i=0; i<=Server[ConP]; i++)
	{
		if(IsPlayerConnected(i)) if(!IsPlayerNPC(i)) SetVehicleParamsForPlayer(Car[series][number], i, 0, 0);
	}
	return 1;
}

