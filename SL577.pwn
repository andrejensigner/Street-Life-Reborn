			GetPlayerName(playerid,Name,24);
			GetPlayerName(id,Name2,24);

			format(MSG1,128,"Nastavil si hráèovi %s (%d) skin èíslo %d.",Name2,id,skinid);
			SendClientMessage(playerid,COLOR_ADMIN,MSG1);
			
			format(MSG1,128,"Administrátor %s (%d) ti nastavil skin èíslo %d.",Name,playerid,skinid);
			SendClientMessage(id,COLOR_ADMIN,MSG1);

			SetPlayerSkin(id,skinid);
			Player[id][Skin]=skinid;
		}
	}
	return 1;
}

