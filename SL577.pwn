		GetPlayerName(playerid,Name,24);

	    if(Player[playerid][Mute]==true)
		{
			format(gBIGSTRING, sizeof(gBIGSTRING), "Si umlèaný, poèkaj ete %d sekúnd kým ti èas trestu vyprí !", (Player[playerid][ToMuteTime] - gettime()));
