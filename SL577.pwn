forward BankSetMoney(playerid, money[]);
public BankSetMoney(playerid, money[])
{
	if(IsPlayerConnected(playerid))
	{
	    if(!strlen(money)) return ShowPlayerDialog(playerid,Dialog_BankSet,DIALOG_STYLE_INPUT,"Banka","{ff0000}Nezadal si èástku !{ffffff}\nZadej èástku kterou chce vloit do banky:","Vloit","Cancel");
	    else if(strval(money) > Player[playerid][Money]) return ShowPlayerDialog(playerid,Dialog_BankSet,DIALOG_STYLE_INPUT,"Banka","{ff0000}Nemá dostatek penezí !{ffffff}\nZadej èástku kterou chce vloit do banky:","Vloit","Cancel");
	    else
		{
	        GivePlayerACMoney(playerid, -strval(money));
	        Player[playerid][Bank] += strval(money);
	        new string[128];
	        format(string,128,"Uspìne si vloil do banky %d$ ! Aktuálni stav konta je: %d$.",strval(money),Player[playerid][Bank]);
	        SendClientMessage(playerid,COLOR_YELLOW,string);
	        Player[playerid][IPIP]=false;
	        PlayerCameraBack(playerid);
	    }
	}
	return 1;
}

forward BankGetMoney(playerid, money[]);
public BankGetMoney(playerid, money[])
{
	if(IsPlayerConnected(playerid))
	{
	    if(!strlen(money)) return ShowPlayerDialog(playerid,Dialog_BankGet,DIALOG_STYLE_INPUT,"Banka","{ff0000}Nezadal si èástku !{ffffff}\nZadej èástku kterou chce vybrat z banky:","Vybrat","Cancel");
	    else if(Player[playerid][Bank] < strval(money)) return ShowPlayerDialog(playerid,Dialog_BankGet,DIALOG_STYLE_INPUT,"Banka","{ff0000}Nemá v bance dostatek peòezí !{ffffff}\nZadej èástku kterou chce vybrat z banky:","Vybrat","Cancel");
	    else
		{
	        GivePlayerACMoney(playerid, strval(money));
	        Player[playerid][Bank] -= strval(money);
	        new string[128];
	        format(string,128,"Uspìne sis vybral z banky %d$ ! Aktuálni stav konta je: %d$.",strval(money),Player[playerid][Bank]);
	        SendClientMessage(playerid,COLOR_YELLOW,string);
	        Player[playerid][IPIP]=false;
	        PlayerCameraBack(playerid);
	    }
	}
	return 1;
}

