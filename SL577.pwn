forward BankSetMoney(playerid, money[]);
public BankSetMoney(playerid, money[])
{
	if(IsPlayerConnected(playerid))
	{
	    if(!strlen(money)) return ShowPlayerDialog(playerid,Dialog_BankSet,DIALOG_STYLE_INPUT,"Banka","{ff0000}Nezadal si èástku !{ffffff}\nZadej èástku kterou chceš vložit do banky:","Vložit","Cancel");
	    else if(strval(money) > Player[playerid][Money]) return ShowPlayerDialog(playerid,Dialog_BankSet,DIALOG_STYLE_INPUT,"Banka","{ff0000}Nemáš dostatek penezí !{ffffff}\nZadej èástku kterou chceš vložit do banky:","Vložit","Cancel");
	    else
		{
	        GivePlayerACMoney(playerid, -strval(money));
	        Player[playerid][Bank] += strval(money);
	        new string[128];
	        format(string,128,"Uspìšne si vložil do banky %d$ ! Aktuálni stav konta je: %d$.",strval(money),Player[playerid][Bank]);
	        SendClientMessage(playerid,COLOR_YELLOW,string);
	        Player[playerid][IPIP]=false;
	        PlayerCameraBack(playerid);
	    }
	}
	return 1;
}

forward BankGetMoney(playerid, money[]);
public BankGetMoney(playerid, money[])
{
	if(IsPlayerConnected(playerid))
	{
	    if(!strlen(money)) return ShowPlayerDialog(playerid,Dialog_BankGet,DIALOG_STYLE_INPUT,"Banka","{ff0000}Nezadal si èástku !{ffffff}\nZadej èástku kterou chceš vybrat z banky:","Vybrat","Cancel");
	    else if(Player[playerid][Bank] < strval(money)) return ShowPlayerDialog(playerid,Dialog_BankGet,DIALOG_STYLE_INPUT,"Banka","{ff0000}Nemáš v bance dostatek peòezí !{ffffff}\nZadej èástku kterou chceš vybrat z banky:","Vybrat","Cancel");
	    else
		{
	        GivePlayerACMoney(playerid, strval(money));
	        Player[playerid][Bank] -= strval(money);
	        new string[128];
	        format(string,128,"Uspìšne sis vybral z banky %d$ ! Aktuálni stav konta je: %d$.",strval(money),Player[playerid][Bank]);
	        SendClientMessage(playerid,COLOR_YELLOW,string);
	        Player[playerid][IPIP]=false;
	        PlayerCameraBack(playerid);
	    }
	}
	return 1;
}

